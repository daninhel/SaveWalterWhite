{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/contador.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/contador",
      "dest": "/api/contador.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}